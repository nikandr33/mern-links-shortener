{
  "name": "mern-links-shortener",
  "version": "1.0.0",
  "description": "Links shortener with mern-stack",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "backend": "nodemon app.js",
    "frontend": "npm run start --prefix client",
    "frontend:install": "npm i --prefix client",
    "frontend:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
